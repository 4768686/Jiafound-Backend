        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.campusitem.mapper.ItemImagesMapper">

    <resultMap id="BaseResultMap" type="org.example.campusitem.entity.ItemImages">
            <id property="imageId" column="image_id" />
