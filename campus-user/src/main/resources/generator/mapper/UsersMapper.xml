<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.campususer.mapper.UsersMapper">

    <resultMap id="BaseResultMap" type="org.example.campususer.entity.Users">
