            <result property="isCertified" column="is_certified" />
            <result property="roleType" column="role_type" />
            <result property="accountStatus" column="account_status" />
            <result property="unfreezeTime" column="unfreeze_time" />
            <result property="coinBalance" column="coin_balance" />
            <result property="frozenBalance" column="frozen_balance" />
            <result property="createTime" column="create_time" />
            <result property="updateTime" column="update_time" />
    </resultMap>

    <sql id="Base_Column_List">
        user_id,open_id,student_id,real_name,nickname,avatar_url,
        email,is_certified,role_type,account_status,unfreeze_time,
        coin_balance,frozen_balance,create_time,update_time
